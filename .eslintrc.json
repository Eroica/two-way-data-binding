{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "plugins": ["jest"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "Proxy": true
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "backtick"]
  }
}